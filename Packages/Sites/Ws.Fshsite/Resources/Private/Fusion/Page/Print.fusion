prototype(Ws.Fshsite:PrintPage) < prototype(Ws.Fshsite:AbstractPage) {
    head {
        stylesheets.printCss = Neos.Fusion:Tag {
            @position = 'end 10'
            tagName = 'link'
            attributes {
                media = 'all'
                rel = 'stylesheet'
                href = Neos.Fusion:ResourceUri {
                    path = 'resource://Ws.Fshsite/Public/Styles/Print.css'
                }
            }
        }
    }
}
